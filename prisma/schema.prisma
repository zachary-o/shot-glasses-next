generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ShotGlass {
  id           Int      @id @default(autoincrement())
  cityEng      String
  cityUkr      String
  countryEng   String
  countryUkr   String
  continentEng String
  continentUkr String
  latitude     Int
  longitude    Int
  imageUrl     String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
